    "class Solution:\n",
    "    def shuffle(self, nums: list[int], n: int) -> list[int]:\n",
    "        left = list(nums[:n])\n",
    "        right = list(nums[n:])\n",
    "        ans = []\n",
    "        for i, j in zip(left, right):\n",
    "            ans.append(i)\n",
    "            ans.append(j)\n",
    "        return ans"
   ]
