    "class Solution:\n",
    "    def subtractProductAndSum(self, n: int) -> int:\n",
    "        hi = list(str(n))\n",
    "        product = 1\n",
    "        summ = 0\n",
    "        for i in hi:\n",
    "            product *= int(i)\n",
    "            summ += int(i)\n",
    "        return product - summ"
   ]
