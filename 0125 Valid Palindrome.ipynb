    "class Solution:\n",
    "    def isPalindrome(self, s: str) -> bool:\n",
    "        s = s.lower().strip()\n",
    "        s = re.sub('\\W','',s)\n",
    "        s = re.sub('[_]', '', s)\n",
    "        if s == s[::-1]:\n",
    "            return True\n",
    "        else:\n",
    "            return False"
