    "class Solution:\n",
    "    def climbStairs(self, n: int) -> int:\n",
    "        if n<2:\n",
    "            return n\n",
    "        x, y = 1, 2\n",
    "        for i in range(n):\n",
    "            temp = x + y\n",
    "            x = y\n",
    "            y = temp\n",
    "            if i == n -2:\n",
    "                return x"
   ]
