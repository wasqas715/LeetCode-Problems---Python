   "source": [
    "class Solution:\n",
    "    def arraySign(self, nums: List[int]) -> int:\n",
    "        ans = 0\n",
    "        for i in nums:\n",
    "            if i < 0:\n",
    "                ans += 1\n",
    "            elif i == 0:\n",
    "                return 0\n",
    "        if ans % 2 == 0:\n",
    "            return 1\n",
    "        else:\n",
    "            return -1"
   ]
