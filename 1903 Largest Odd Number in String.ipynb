   "source": [
    "def largestOddNumber(num: str):\n",
    "    for i in range(len(num)-1, -1, -1):\n",
    "        if int(num[i]) % 2 != 0:\n",
    "            return(num[:i+1])\n",
    "    return \"\"\n"
   ]
