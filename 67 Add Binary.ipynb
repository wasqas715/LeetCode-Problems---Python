    "class Solution:\n",
    "    def addBinary(self, a: str, b: str) -> str:\n",
    "        decimal_sum = int(a, 2) + int(b, 2)\n",
    "        binary_sum = bin(decimal_sum)[2:]\n",
    "        return binary_sum"
   ]
