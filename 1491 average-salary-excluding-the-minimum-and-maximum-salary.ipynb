    "class Solution:\n",
    "    def average(self, salary: list[int]) -> float:\n",
    "        salary = sorted(salary)\n",
    "        salary = salary[1:-1]\n",
    "        count = 0\n",
    "        for i in salary:\n",
    "            count += i\n",
    "        avg = count / len(salary)\n",
    "        return avg"
   ]
  }
