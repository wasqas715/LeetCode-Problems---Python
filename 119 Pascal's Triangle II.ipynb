    "class Solution:\n",
    "    def getRow(self, rowIndex: int) -> list[int]:\n",
    "        final = []\n",
    "        n = rowIndex\n",
    "        for r in range(n+1):\n",
    "            temp = int((math.factorial(n))/((math.factorial(r)) * (math.factorial(n-r))))\n",
    "            final.append(temp)\n",
    "        return (final)"
   ]
