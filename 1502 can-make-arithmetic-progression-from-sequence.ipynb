   "source": [
    "class Solution:\n",
    "    def canMakeArithmeticProgression(self, arr: List[int]) -> bool:\n",
    "        arr = sorted(arr)\n",
    "        spacing = arr[1] - arr[0]\n",
    "        for i in range(len(arr) - 1):\n",
    "            if arr[i+1] - arr[i] != spacing:\n",
    "                return False\n",
    "        return True"
   ]
  }
